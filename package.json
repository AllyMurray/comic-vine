{
  "name": "comic-vine",
  "version": "0.0.0",
  "description": "A JS/TS client for the Comic Vine API",
  "keywords": [
    "comic",
    "comic-vine",
    "metadata"
  ],
  "homepage": "https://github.com/AllyMurray/comic-vine#readme",
  "bugs": {
    "url": "https://github.com/AllyMurray/comic-vine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllyMurray/comic-vine.git"
  },
  "license": "MIT",
  "author": "Ally Murray",
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.cjs.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "run-s build:**",
    "build:clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:package-json": "bash ./scripts/create-package-json",
    "dev": "npm run build -- --watch src",
    "test": "jest",
    "format": "prettier ./src --write",
    "lint": "eslint src --fix",
    "prepare": "husky install",
    "prepack": "npm run build"
  },
  "dependencies": {
    "axios": "0.27.2",
    "clone-deep": "4.0.1"
  },
  "devDependencies": {
    "@types/clone-deep": "4.0.1",
    "@types/jest": "29.1.1",
    "@types/node": "16.11.59",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "nock": "13.2.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{ts,json,md}": "prettier --write"
  }
}
