{
  "name": "comic-vine",
  "version": "0.0.0",
  "description": "A JS/TS client for the Comic Vine API",
  "keywords": [
    "comic",
    "comic-vine",
    "metadata"
  ],
  "homepage": "https://github.com/AllyMurray/comic-vine#readme",
  "bugs": {
    "url": "https://github.com/AllyMurray/comic-vine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllyMurray/comic-vine.git"
  },
  "license": "MIT",
  "author": "Ally Murray",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean --dts --treeshake --minify",
    "build:watch": "npm run build -- --watch src",
    "test": "jest",
    "format": "prettier ./src --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "0.27.2",
    "clone-deep": "4.0.1"
  },
  "devDependencies": {
    "@types/clone-deep": "4.0.1",
    "@types/jest": "29.1.1",
    "@types/node": "16.11.59",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "nock": "13.2.9",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*.{ts,json,md}": "prettier --write"
  }
}
